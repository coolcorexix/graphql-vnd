{
  "plugins": [
    [
      "module-name-mapper",
      {
        "moduleNameMapper": {
          "^mapper-gql/(.*)": "<pkgDir>/src/api/graphql/$1",
          "^mapper-api/(.*)": "<pkgDir>/src/api/$1"
        }
      }
    ],
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true,
      },
    ],
    [
      "babel-plugin-root-import", {
        "rootPathSuffix": "src/"
      }
    ],
    "@babel/plugin-proposal-object-rest-spread"
  ],
  "presets": [
    "@babel/preset-env"
  ]
}
  